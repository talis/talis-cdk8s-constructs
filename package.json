{
  "name": "talis-cdk8s-constructs",
  "version": "0.1.0",
  "private": true,
  "files": [
    "imports/**/*.js",
    "imports/**/*.d.ts",
    "lib/**/*.d.ts",
    "lib/**/*.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "npm": ">=8.1.2",
    "node": ">=16.13.2"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "import": "cdk8s import --language typescript",
    "format:prettier": "prettier --write .",
    "format": "npm run format:prettier",
    "lint:eslint": "eslint . --ext .ts",
    "lint:prettier": "prettier --check .",
    "lint:ts": "tsc --noEmit",
    "lint": "npm run lint:prettier && npm run lint:eslint && npm run lint:ts",
    "prepare": "husky install && ./e2e/install-detik.sh",
    "release": "npm run build && semantic-release",
    "e2e": "bats --jobs 2 --no-parallelize-within-files --timing --print-output-on-failure e2e/",
    "test": "jest"
  },
  "peerDependencies": {
    "cdk8s": "^2.3.35",
    "constructs": "^10.1.42"
  },
  "dependencies": {
    "cron-parser": "4.4.0",
    "dotenv": "^14.3.2",
    "sort-keys": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/jest": "^28.1.3",
    "@types/lodash": "^4.14.182",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "bats": "^1.7.0",
    "cdk8s": "^2.3.35",
    "cdk8s-cli": "^2.0.30",
    "constructs": "^10.1.42",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-junit": "^13.2.0",
    "lint-staged": "^13.0.2",
    "lodash": "^4.17.21",
    "mock-fs": "^5.1.2",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  }
}
